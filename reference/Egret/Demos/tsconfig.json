{
	"compilerOptions": {
		"watch": false,
		"sourceMap": false,
		"declaration": false,
		"alwaysStrict": true,
		"noImplicitAny": false,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"strictNullChecks": false,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"target": "es5",
		"lib": [
			"es5",
			"dom",
			"es2015.promise"
		]
	},
	"exclude": [
		"node_modules",
		"out"
	]
}